x=float(input("Ingrese el valor del primer lado: "))
y=float(input("Ingrese el valor del segundo lado: "))
z=float(input("Ingrese el valor del tercer lado: "))
while (x and y and z) <= 0:
  print("ingrese medidas validas")
  x=float(input("Ingrese el valor del primer lado: "))
  y=float(input("Ingrese el valor del segundo lado: "))
  z=float(input("Ingrese el valor del tercer lado: "))

if x==y and y==z:
  print("Su triangulo es equilatero")
elif (x==y and z!=x) or (x==z and y!=x) or (z==y and z!=x):
  print("su triangulo es isosceles")
else:
  print("su triangulo es escaleno")
